{
    /**Bundle configiration. Module build config is in gulp file*/
    "compilerOptions": {
        "target": "ES5",
        "module": "ES2015",
        "lib": ["ES2015","dom"],
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "allowJs": true,
        "esModuleInterop": true,
    }

}